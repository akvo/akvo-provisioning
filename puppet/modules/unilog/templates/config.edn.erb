{
    ;; path to akvo-flow-server-config
    :config-folder "<%= @flow_server_config %>"
    :event-schema-file "<%= @flow_data_schema %>"

    ;; port to run the service
    :port <%= @appport %>

    ;; postgreSQL host, port, username and password
    :database-host "<%= @postgres_host %>"
    :database-port "<%= @postgres_port %>"
    :database-user "<%= @username %>"
    :database-password "<%= @database_password %>"

    ;; remote api e-mail and password
    :remote-api-email "<%= @remote_api_email %>"
    :remote-api-password "<%= @remote_api_password %>"
<% if @use_sentry %>
    ;; sentry dsn
    :sentry-dsn "<%= @sentry_dsn %>"
<% end %>
<% if @use_statsd %>
    ;; statsd config
    :statsd-host "<%= @statsd_host %>"
    :statsd-port "<%= @statsd_port %>"
    :statsd-prefix "<%= @statsd_prefix %>"
<% end %>
}
